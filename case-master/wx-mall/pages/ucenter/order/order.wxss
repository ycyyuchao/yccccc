.service {
  width:100%;
  height: 100vh;
  background: #f9f9f9;
}
.serviceList {
  width:100%;
  border-top:3rpx solid #ccc;

}
.serviceItem {
  width: 100%;
  padding:20rpx;
  border-bottom:3rpx solid #ccc;
  background: #fff;
  
}
.itemList{
  overflow: hidden;
  clear: both;
  margin-left:10rpx;
}
.serviceImg {
  width:120rpx;
  height: 120rpx;
  float: left;
}
.serviceData {
  width: 80%;
  float: right;
}
.serviceTop{
  overflow: hidden;
  clear: hidden;
  width: 80%;
}
.serviceName {
  float: left;
  font-size: 28rpx;
}
.serviceState{
  float: right;
  color:#ffb45c;
  font-size: 36rpx;
}

.serviceTime{
  font-size:24rpx;
  margin-top:10rpx;
}
.serviceTotal {
  width: 50%;
  font-size: 28rpx;
  margin-left: 60%;
}
.serviceCost {
  font-size: 36rpx;
  color:#ffb45c;
  display: inline-block;
 
}
.tab {
  width: 100%;
  display: flex;
  justify-content: space-around;
  padding:30rpx 0;
  border-bottom:4rpx solid rgb(244, 242, 242);
}
.tab {
  font-size: 32rpx;
  font-weight: 400;

}

.on {
  color:#f00;
  font-weight: 600;
}


